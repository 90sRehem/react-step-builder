{
  "name": "react-step-builder",
  "description": "Unopinionated multi step interface builder.",
  "author": "Samet Mutevelli <mutevellisamet@gmail.com> (https://sametmutevelli.com)",
  "version": "1.1.5",
  "private": false,
  "devDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.1",
    "@babel/runtime": "^7.8.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "SET NODE_ENV=production&babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__"
  },
  "keywords": [
    "step-builder",
    "react-step-builder",
    "create steps in react",
    "step by step form",
    "multi step form",
    "react multi step registration"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/index",
  "typings": "dist/index",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sametweb/react-step-builder"
  }
}
